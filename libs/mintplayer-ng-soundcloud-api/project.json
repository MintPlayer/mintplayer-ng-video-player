{
  "name": "mintplayer-ng-soundcloud-api",
  "$schema": "..\\..\\node_modules\\nx\\schemas\\project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/mintplayer-ng-soundcloud-api/src",
  "prefix": "mintplayer-ng-video-player",
  "targets": {
    "build": {
      "executor": "@nx/angular:package",
      "outputs": ["{workspaceRoot}/dist/libs/mintplayer-ng-soundcloud-api"],
      "options": {
        "project": "libs/mintplayer-ng-soundcloud-api/ng-package.json",
        "tsConfig": "libs/mintplayer-ng-soundcloud-api/tsconfig.lib.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/mintplayer-ng-soundcloud-api/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/mintplayer-ng-soundcloud-api/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/mintplayer-ng-soundcloud-api"],
      "options": {
        "codeCoverage": true,
        "coverageReporters": ["lcovonly"],
        "coverageDirectory": "coverage/libs/mintplayer-ng-soundcloud-api",
        "jestConfig": "libs/mintplayer-ng-soundcloud-api/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/mintplayer-ng-soundcloud-api/src/**/*.ts",
          "libs/mintplayer-ng-soundcloud-api/src/**/*.html"
        ]
      }
    }
  },
  "tags": []
}
