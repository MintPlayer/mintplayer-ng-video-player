{
  "name": "mintplayer-dailymotion-player",
  "$schema": "..\\..\\node_modules\\nx\\schemas\\project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/mintplayer-dailymotion-player/src",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/mintplayer-dailymotion-player",
        "main": "libs/mintplayer-dailymotion-player/src/index.ts",
        "tsConfig": "libs/mintplayer-dailymotion-player/tsconfig.lib.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/mintplayer-dailymotion-player/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/mintplayer-dailymotion-player/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["coverage/libs/mintplayer-dailymotion-player"],
      "options": {
        "codeCoverage": true,
        "coverageReporters": ["lcovonly"],
        "coverageDirectory": "coverage/libs/mintplayer-dailymotion-player",
        "jestConfig": "libs/mintplayer-dailymotion-player/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    }
  },
  "tags": []
}
