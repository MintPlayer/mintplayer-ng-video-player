{
  "name": "mintplayer-ng-script-loader",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/mintplayer-ng-script-loader/src",
  "prefix": "mintplayer-ng-video-player",
  "tags": [],
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/angular:package",
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "options": {
        "project": "libs/mintplayer-ng-script-loader/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/mintplayer-ng-script-loader/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/mintplayer-ng-script-loader/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "codeCoverage": true,
        "coverageReporters": ["lcovonly"],
        "coverageDirectory": "coverage/libs/mintplayer-ng-script-loader",
        "jestConfig": "libs/mintplayer-ng-script-loader/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/mintplayer-ng-script-loader/**/*.ts",
          "libs/mintplayer-ng-script-loader/**/*.html",
          "libs/mintplayer-ng-script-loader/package.json"
        ]
      }
    }
  }
}
